{
	"name": "aksel",
	"version": "0.1.0",
	"description": "An AI agent framework for typescript",
	"keywords": [
		"AI agents",
		"typescript",
		"AI agent framework"
	],
	"main": "dist/index.js",
	"homepage": "https://github.com/iyifr/aksel",
	"bugs": {
		"url": "https://github.com/iyifr/aksel/issues"
	},
	"author": "Adekile Iyimide",
